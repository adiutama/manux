#!/usr/bin/env node

const app = require('commander')
const Project = require('../core/project')
const { log } = console

/**
 * @type {Project}
 */
const project = Project.getInstance()

/**
 * Command action
 *
 * @param {string} name
 */
const action = name => {
  project.rm(name)

  log('')
  log('Done!')
}

app
  .action(action)
  .parse(process.argv)
